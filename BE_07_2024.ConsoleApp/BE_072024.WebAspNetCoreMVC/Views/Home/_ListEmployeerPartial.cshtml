@model List<BE_072024.WebAspNetCoreMVC.Models.BE072024_HB_Rooms>
<table class="table">
    <thead>
        <tr>
            <th>RoomID</th>
            <th>RoomNumber</th>
            <th>RoomSquare</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Count > 0)
        {
            foreach (var item in Model)
            {
                <tr>
                    <td>@item.RoomID</td>
                    <td>@item.RoomNumber</td>
                    <td>@item.RoomSquare</td>
                </tr>
            }
        }
        else
        {
            @if (!string.IsNullOrEmpty(ViewBag.Message))
            {
                <tr>
                    <td colspan="3" style="color:red;font-style:italic;">@ViewBag.Message</td>

                </tr>
            }
            else
            {
                <tr>
                    <td colspan="3">Không tồn tại dữ liệu</td>

                </tr>
            }
        }

    </tbody>
</table>